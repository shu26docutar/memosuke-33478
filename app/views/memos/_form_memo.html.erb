    
    <%= form_with(model: [@room, @memo], id: "memo-form") do |form| %>
        <div class="form">
            <%= form.text_field :content, id: "content" %>
            <%= form.submit '保存', id: "submit" %>
        </div>
        <input name="room[user_ids][]" type="hidden" value=<%= current_user.id %>>
    <% end %>
